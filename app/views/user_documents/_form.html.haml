%h3= 'Documento probatorio'
- blueprint('span-20 prepend-2 last', :id => 'collection_tr') do
  = f.collection_select :document_id, Document.all, :id, :name, :blueprint => 'span-6 prepend-3'
  = f.file_field :file, :size => 20, :blueprint => 'span-10 last'
- blueprint 'span-10 prepend-2 last', :id => 'buttons' do
  = f.submit(t(:save))
  = ' | '
  = link_to t(:cancel), file_uploaders_path
